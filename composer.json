{
    "name": "abedi-ir/wordpress-flysystem-s3",
    "description": "Upload wordpress media files to s3 object storage",
    "type": "library",
    "require": {
        "php": "^8",
        "league/flysystem-aws-s3-v3": "^2",
        "doctrine/dbal": "^5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mahdi Abedi",
            "email": "abedi@bluc.ir"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Abedi\\WPFlysystemS3\\": "src/"
        }
    },
    "require-dev": {
        "humbug/box": "^4"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}
